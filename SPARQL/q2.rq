PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX perodicTable: <http://www.daml.org/2003/01/periodictable/PeriodicTable#>

SELECT ?element
WHERE {
  ?element rdf:type perodicTable:Element .
  ?element perodicTable:standardState perodicTable:liquid .
  ?element perodicTable:classification perodicTable:Metallic .
}
